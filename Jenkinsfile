pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                    echo "Compile the code"
            }
        }
            
        stage('Testing') {
            steps {
                    echo "Testing the code"
            }
        }
            
        stage('Build') {
            steps {
                    echo "Building the code"
            }

        }
    }
}
